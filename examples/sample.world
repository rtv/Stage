#end 


begin position_device
  name 6665 
  port 6665 
  pose (9.750 8.950 0.000) 
  #shape circle 
  #color blue 
  begin player_device
    pose (0.000 0.000 0.000) 
  end 
  begin gps_device
    pose (0.000 0.000 0.000) 
  end 
  begin laser_device
    pose (0.000 0.000 0.000) 
    laser false 
    begin lbd_device
      pose (0.000 0.000 0.000) 
    end 
    begin laser_beacon
      id 6665 
      laser true 
      laser_bright true 
      pose (0.000 0.000 0.000) 
    end 
  end 
end 


begin omni_position_device
  name 6666 
  port 6666 
  pose (9.000 7.400 0.000) 
  #shape circle 
  #color blue 
  begin player_device
    pose (0.000 0.000 0.000) 
  end 
  begin gps_device
    pose (0.000 0.000 0.000) 
  end 
  begin laser_device
    pose (0.000 0.000 0.000) 
    laser false 
    begin lbd_device
      pose (0.000 0.000 0.000) 
    end 
    begin laser_beacon
      id 6666 
      laser true 
      laser_bright true 
      pose (0.000 0.000 0.000) 
    end 
  end 
end 
