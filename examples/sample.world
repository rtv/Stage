begin environment
  file hospital.pnm 
  scale 0.05 
  #crop (30 0 50 20)
  resolution 0.10 
end 

begin rtk
  size (640 480) 
  scale 0.05 
end 

laser_min_res 0.5 
laser_scan_rate 7200 

lbd_range_anon 4 
lbd_range_id 4 


