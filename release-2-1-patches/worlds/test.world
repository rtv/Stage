# defines Pioneer-like robots
include "pioneer.inc"


size [20 20]

# defines 'map' object used for floorplans
include "map.inc"



# set the size of a pixel in meters
resolution 0.05

# configure the GUI window
window
( 
  size [ 731.000 763.000 ] 
  center [0.161 -0.057] 
  scale 0.028 
)

# load an environment bitmap
map
(
  bitmap "bitmaps/cave.png"
  #bitmap "bitmaps/hospital.png"
  size [16 16]
  boundary 1
  name "cave"
)

