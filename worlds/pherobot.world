
# Desc: an environment full of pherobots
# works well with Player's C++ client pherobot example.
# use the supplied script to run many clients:
# 
# (PLAYER_DIR)/examples/c++/startmany.tcl pherobot 100
#
# CVS: $Id: pherobot.world,v 1.11.8.1 2004-11-11 19:27:25 gerkey Exp $


resolution 0.02

bitmap
(
  file "hospital_section.pnm.gz"
  #file "cave.pnm.gz"
  resolution 0.02
) 

gui
(
  size [945.000 582.000]
  origin [6.037 5.160]
  scale 0.013
  grid [0.2 1]
  showgrid 1
)


define pherobot position
( 
 shape "circle"
 size [0.12 0.12]
 
 #idarturret() 

 # 2 almost semi-circular bumpers, left and right, overlapping
 # slightly in front with a dead spot behind
 #	
 # bumper pose format is [x y th length radius]
 # if radius is zero then the bumper is a straight line
 #	
 bumper
 ( 
   bcount 2 
   bpose[0] [ 0.0   0.01 -80 0.22 0.08 ]
   bpose[1] [ 0.0  -0.01 +80 0.22 0.08 ]
 ) 
)

